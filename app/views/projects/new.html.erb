<h1>Create a new project%></h1>

<%= form_for @project do |u| %>
    <div>
    <%= u.label :name %>
    <%= u.text_field :name %>
    </div>
    <div>
    <%= u.label :github_url %>
    <%= u.text_field :github_url %>
    </div>
    <div>
    <%= u.label :description %>
    <%= u.text_field :description %>
    </div>
    <%= u.submit %>
<% end %>